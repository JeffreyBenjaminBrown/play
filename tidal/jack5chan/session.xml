<!DOCTYPE qjackctlSession>
<session name="jack5chan">
 <client name="dirt">
  <port type="out" name="output_0">
   <connect port="playback_1" client="system"/>
  </port>
  <port type="out" name="output_1">
   <connect port="playback_1" client="system"/>
  </port>
  <port type="out" name="output_2">
   <connect port="playback_2" client="system"/>
  </port>
  <port type="out" name="output_3">
   <connect port="playback_2" client="system"/>
  </port>
  <port type="out" name="output_4">
   <connect port="playback_2" client="system"/>
  </port>
 </client>
 <client name="system">
  <port type="out" name="capture_1"/>
  <port type="out" name="capture_2"/>
  <port type="in" name="playback_1">
   <connect port="output_0" client="dirt"/>
   <connect port="output_1" client="dirt"/>
  </port>
  <port type="in" name="playback_2">
   <connect port="output_2" client="dirt"/>
   <connect port="output_3" client="dirt"/>
   <connect port="output_4" client="dirt"/>
  </port>
 </client>
</session>

